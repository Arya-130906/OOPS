class A{
     String name;
     int id;
   
     public A(String name, int id) {
         this.name = name;
         this.id = id;
     }
 }
 class ABC{
 public static void main(String[] args)
     {
         A aa = new A("Arya" , 74);
         System.out.println(aa.name);
         System.out.println(aa.id);
     }
 }
