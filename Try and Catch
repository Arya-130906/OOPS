public class A {
     public static void main(String[] args)
     {
         int n = 10;
         int m = 0;
         try {
             int ans = n / m;
             System.out.println(ans);
         }
         catch (ArithmeticException e) {
             System.out.println("Division not allowed.");
         }
             System.out.println("Program continues after handling the exception.");
     }
}
 
