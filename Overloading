public class A {
     public static int B(int a,int b) {
         int c=a+b;
         return c;
     }
     public static int H(int f,int d,int e) {
         int g=f*d*e;
         return g;
     }
     public static double Y(int t,double u,double i){
         double k=t+u+i;
         return k;
     }
     public static void main(String args[])
     { 
         System.out.println(B(8,5));
         System.out.println(H(5,8,6));
         System.out.println(Y(5,2.5,1.2));
     }
}
